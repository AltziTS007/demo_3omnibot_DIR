	<xacro:include filename="$(find demo_navi)/src/urdf/xacro/rim.urdf.xacro"/>
  <xacro:include filename="$(find demo_navi)/src/urdf/xacro/common_materials.xacro" />

	<link name="base_footprint"/>

	<link name="base_link">

	<inertial>
		<origin xyz="-0.147 0 0"
			rpy="0 0 0" />
		<mass value="5"/>
		<inertia
			ixx="0.15"
			ixy="0"
			ixz="0"
			iyy="0.104167"
			iyz="0"
			izz="0.254167"
		/>

	</inertial>
	<visual>
		<origin xyz="0 0 0"
			rpy="0 0 0" />

		<geometry>

			<mesh filename="package://demo_navi/src/meshes/base_link.stl"/>

		</geometry>
		<material name="base_material">

			<color rgba="0.098039 0.098039 0.098039 1"/>

		</material>

	</visual>
	<collision>
		<origin xyz="0 0 0"
			rpy="0 0 0" />

		<geometry>

			<mesh filename="package://demo_navi/src/meshes/base_link.stl"/>

		</geometry>

	</collision>

	</link>
	<joint
		name="base_joint"
		type="fixed"
	>

		<parent link="base_footprint" />
		<child link="base_link" />
		<origin
			xyz="0 0 0"
		/>

	</joint>
	<xacro:rim_macro
		name="left"
		color="0.3 0 0"
		xyz="-0.0267 0.214 0.04"
		rpy="0 0 -2.0944"
	/>
	<xacro:rim_macro
