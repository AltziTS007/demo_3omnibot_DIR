        </geometry>
      </collision>
      <inertial>
        <mass value="0.001" />
        <origin xyz="0 0.27 0.07" />
        <inertia ixx="0.000001" ixy="0.0" ixz="0.0"
          iyy="0.000001" iyz="0.0" izz="0.000001" />
      </inertial>
    </link>

