	<link name="base_footprint"/>

  <link name="imu_link"/>

	<link name="base_link">

	<inertial>
		<origin xyz="-0.147 0 0.05"
			rpy="0 0 0" />
		<mass value="15"/>
		<inertia
			ixx="15.0"
			ixy="0"
			ixz="0"
			iyy="15.0"
			iyz="0"
			izz="9.0"
		/>

	</inertial>
	<visual>
		<origin xyz="0 0 0"
			rpy="0 0 0" />

		<geometry>

			<mesh filename="package://demo_navi/src/meshes/base_link.stl"/>

		</geometry>
		<material name="base_material">

			<color rgba="0.098039 0.098039 0.098039 1"/>

		</material>

	</visual>
	<collision>
		<origin xyz="0 0 0"
			rpy="0 0 0" />

		<geometry>

			<mesh filename="package://demo_navi/src/meshes/base_link.stl"/>

		</geometry>

	</collision>

	</link>
	<joint
		name="base_joint"
		type="fixed"
	>

		<parent link="base_footprint" />
		<child link="base_link" />
		<origin
			xyz="0 0 0"
		/>

	</joint>

    <joint
    name="imu_joint"
    type="fixed"
  >

    <parent link="base_footprint" />
    <child link="imu_link"/>
