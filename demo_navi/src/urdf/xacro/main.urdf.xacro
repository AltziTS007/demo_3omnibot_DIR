	/>


    <joint name="sick_joint_R" type="fixed">
      <parent link="base_link" />
      <child link="sick_R_laser" /><!-- allakse to adress -->
      <origin rpy="-3.1415 0 -1.0472" xyz="0.008 -0.275 0.042"/>
    </joint>

    <link name="sick_R_laser">
